{
  "plugins": ["react-hooks"],
  "rules": {
    "additional-rule": "warn",
    "@typescript-eslint/ban-ts-ignore": "off",
    "react-hooks/exhaustive-deps": "off"
  },
  "parserOptions": {
    "rules": {
      "no-underscore-dangle": [
          "error",
          {
              "allow": ["_id", "b_codes_id"]
          }
      ],
      "react/prop-types": [1],
      "react-hooks/exhaustive-deps": "off"
    }
  }
}
